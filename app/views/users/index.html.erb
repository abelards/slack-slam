<div class="row">
  <div class="col-md-12">
    <h1>Users</h1>
  </div>
</div>

<div class="row">
  <div class="col-md-12">
    <table id="userlist" class="table table-striped table-bordered" cellspacing="0" width="100%">

      <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= link_to(user.slack_name, user_path(user)) %></td>
        </tr>
      <% end %>
      </tbody>
    </table>
  </div>
</div>
